<apex:page controller="ApexTestResultLimitsListController" lightningStylesheets="true">
    <h1>ApexTestResultLimits の内容をただ確認したいだけ</h1>
    <apex:pageBlock title="ApexTestResultLimits">
        <apex:pageBlockTable value="{!testResultLimits}" var="item">
            <apex:column value="{!item.ApexTestResultId}"/>
            <apex:column value="{!item.AsyncCalls}"/>
            <apex:column value="{!item.Callouts}"/>
            <apex:column value="{!item.Cpu}"/>
            <apex:column value="{!item.Dml}"/>
            <apex:column value="{!item.DmlRows}"/>
            <apex:column value="{!item.Email}"/>
            <apex:column value="{!item.LimitContext}"/>
            <apex:column value="{!item.LimitExceptions}"/>
            <apex:column value="{!item.MobilePush}"/>
            <apex:column value="{!item.QueryRows}"/>
            <apex:column value="{!item.Soql}"/>
            <apex:column value="{!item.Sosl}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>